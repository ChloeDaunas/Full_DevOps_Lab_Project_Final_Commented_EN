{
  "name": "devops-lab-student-project",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint ."
  },
  "dependencies": {
    "express": "^4.19.2"
  },
  "devDependencies": {
    "vitest": "^2.0.5",
    "@vitest/coverage-v8": "^2.0.5",
    "supertest": "^7.0.0",
    "nodemon": "^3.1.7",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.6.0"
  },
  "vitest": {
    "coverage": {
      "provider": "v8",
      "all": true,
      "include": [
        "src/**/*.js"
      ],
      "lines": 80,
      "functions": 80,
      "branches": 70,
      "statements": 80
    }
  },
  "description": "",
  "main": "main.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChloeDaunas/Full_DevOps_Lab_Project_Final_Commented_EN.git"
